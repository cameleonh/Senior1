## 데이터 불러오기
dataset1 <- read_excel("Lt08.xlsx") ##%>% as.character()
dataset2 <- read_excel("str08.xlsx") ##%>% as.character()
dataset3 <- read_excel("w08.xlsx") ##%>% as.character()
View(dataset)
str(dataset1)
